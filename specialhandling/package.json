    "Rick Leverton <rleverton@email.arizona.edu>"
  ],
  "repository": {
    "type": "git",
    "url": "https://gitlab.fso.arizona.edu/FAST/specialhandling"
  },
  "version": "1.0.0",
  "description": "Paperless auditing of checks that require signatures to be picked up",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "devDependencies": {
    "@vue/test-utils": "1.0.0-beta.24",
    "ag-grid": "^17.1.1",
    "ag-grid-vue": "^17.1.0",
    "animate.css": "^3.6.1",
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^7.2.5",
    "axios": "^0.21.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-jest": "^23.0.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-runtime": "^6.25.0",
    "bootstrap": "git+https://gitlab.fso.arizona.edu/FAST/fso-bootstrap.git#v0.1.4",
    "bootstrap-loader": "^2.1.0",
    "chromedriver": "^2.40.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.19.1",
    "eslint-plugin-jest": "^21.4.2",
    "eslint-plugin-vue": "^4.5.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "flush-promises": "^1.0.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "imports-loader": "^0.7.1",
    "jest": "^23.0.0",
    "jest-mock-axios": "^2.1.11",
    "jest-serializer-vue": "^2.0.0",
    "jquery": "^3.2.1",
    "moment": "^2.22.1",
    "nightwatch": "^0.9.21",
    "node-sass": "^4.11.0",
    "postcss-loader": "^2.0.6",
    "progress-bar-webpack-plugin": "^1.10.0",
    "raven-js": "^3.20.0",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "sass-resources-loader": "^1.3.0",
    "script-loader": "^0.7.0",
    "style-loader": "^0.18.2",
    "tether": "^1.4.0",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "url-loader": "^0.5.9",
    "v-tooltip": "^2.0.0-rc.32",
    "vue": "2.5.9",
    "vue-content-placeholders": "^0.2.1",
    "vue-html-loader": "^1.2.4",
    "vue-jest": "^2.6.0",
    "vue-loader": "^13.0.4",
    "vue-router": "2.7.0",
    "vue-signature": "^2.1.1",
    "vue-template-compiler": "2.5.9",
    "vueoom": "git+https://gitlab.fso.arizona.edu/FAST/vueoom.git#1.1.2",
    "vuex": "^3.0.0",
    "vuex-router-sync": "4.1.0",
    "webpack": "^3.8.1",
    "webpack-bundle-tracker": "^0.3.0",
    "webpack-config-utils": "^2.3.0"
  },
  "scripts": {
    "build": "webpack --config app/build/webpack.config.babel.js --env.production --define process.env.NODE_ENV=production",
    "dev": "webpack --config app/build/webpack.config.babel.js --env.development --define process.env.NODE_ENV=development --hot=true --color --progress",
    "clear-static": "rm -rf specialhandling/static/js/**.*",
    "nom": "rm -rf node_modules",
    "lint": "eslint --ext .js,.vue app/* --config .eslintrc.js --ignore-path .eslintignore --format codeframe --color",
    "lint:html": "eslint --ext .js,.vue app/* --config .eslintrc.js --ignore-path .eslintignore --format html --output-file eslintlog.html",
    "test": "npm run unit && npm run e2e",
    "unit": "jest --config app/tests/unit/jest.config.js",
    "unit:coverage": "jest --config app/tests/unit/jest.config.js --coverage --colors --no-cache",
    "unit:watch": "jest --config app/tests/unit/jest.config.js --watch",
    "e2e": "nightwatch --config app/tests/e2e/nightwatch.config.js --env default"
  },
  "license": "MIT"
}
